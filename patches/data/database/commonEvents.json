  "trial-timer-ten": {
  	"frequency": "ALWAYS",
    "repeat": "REPEAT",
    "eventType": "PARALLEL",
    "overrideSideMessage": false,
  	"event": [
	    {
	      "volume": 1.5,
	      "name": "",
	      "loop": false,
	      "offset": 0,
	      "startTime": 0,
	      "speed": 1.35,
	      "type": "PLAY_SOUND",
	      "sound": "media/sound/scenes/swoosh-rise-short.ogg"
	    },
	    {
	    	"count": 10,
	    	"type":"SHOW_TRIAL_TIME_REMAINING_GUI"
	    },
	    {
        "changeType": "set",
        "type": "CHANGE_VAR_NUMBER",
        "varName": "tmp.ticks",
        "value": 10
      },
      {
      	"name":"countdown",
      	"type":"LABEL"
      },
      {
      	"time":1,
      	"type":"WAIT"
      },
      {
      	"condition":"tmp.ticks < 10 && tmp.ticks % 2 == 0",
      	"withElse":true,
      	"type":"IF",
      	"thenStep":[
      		{
			      "volume": 1.05,
			      "name": "",
			      "loop": false,
			      "offset": 0,
			      "startTime": 0,
			      "speed": 1,
			      "type": "PLAY_SOUND",
			      "sound": "media/sound/arena/trial-tick-1.ogg"
			    }
      	],
      	"elseStep":[
      		{
			      "volume": 1.05,
			      "name": "",
			      "loop": false,
			      "offset": 0,
			      "startTime": 0,
			      "speed": 1,
			      "type": "PLAY_SOUND",
			      "sound": "media/sound/arena/trial-tick-2.ogg"
			    }
      	]
      },
      {
        "changeType": "sub",
        "type": "CHANGE_VAR_NUMBER",
        "varName": "tmp.ticks",
        "value": 1
      },
      {
      	"name":"countdown",
      	"condition":"tmp.ticks > 0 && !tmp.roundFailedPre && !tmp.bossKillHit",
      	"type":"GOTO_LABEL_WHILE"
      }
  	],
    "type": {
      "type": "CALL"
    }
  }
